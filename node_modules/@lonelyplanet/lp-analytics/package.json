{
  "name": "@lonelyplanet/lp-analytics",
  "version": "2.2.6",
  "description": "A JS library providing a standard interface for event tracking for analytics services across LP's frontend codebases.",
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "npx rimraf ./dist && npx tsc && npx webpack",
    "build:tsc": "npx rimraf ./dist && npx tsc",
    "lint": "npx tslint --project .",
    "test": "npm run lint && npx jest",
    "tslint": "tslint -p tsconfig.json -c tslint.json './src/**/*.ts' './src/**/*.tsx'",
    "upload_static": "node ./upload-static-assets.js",
    "update_static": "npm run build && npm run upload_static",
    "version": "npm run build && npx standard-version",
    "version:patch": "npm run version -- --release-as patch",
    "version:minor": "npm run version -- --release-as minor",
    "version:major": "npm run version -- --release-as major",
    "preversion": "npm run test && npm run build",
    "push": "git push --follow-tags origin develop && npm publish"
  },
  "dependencies": {
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "devalue": "^1.0.4",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "@types/core-js": "^2.5.0",
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.3",
    "@types/react-helmet": "^5.0.7",
    "@types/react-side-effect": "^1.1.1",
    "aws-sdk": "^2.332.0",
    "compression-webpack-plugin": "^2.0.0",
    "core-js": "^2.5.7",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "jest": "^23.6.0",
    "prettier": "1.14.3",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-side-effect": "^1.0.0",
    "rimraf": "^2.6.2",
    "standard-version": "^4.4.0",
    "ts-jest": "^23.10.0",
    "ts-loader": "^5.1.1",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.0.3",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-side-effect": "*"
  }
}
