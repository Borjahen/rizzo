{
  "name": "@lonelyplanet/dotcom-core",
  "version": "2.14.0",
  "bin": {
    "dcc": "./bin/dcc.js"
  },
  "scripts": {
    "build": "rm -rf .tmp dist dist-es lib && npm run build:babel && npm run build:es",
    "build:babel": "./bin/babel-build",
    "build:dist": "tsc -p tsconfig-build.json",
    "build:es": "tsc -p tsconfig-es.json",
    "build:webpack": "webpack --config webpack.prod.ts",
    "dev": "node -r ts-node/register bin/devserver",
    "prepublishOnly": "npm run build",
    "upload": "npm run build:webpack && npm run upload:run",
    "upload:run": "aws-mfa node dist/scripts/upload.js",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "@lonelyplanet/open-planet-node": "^2.24.4",
    "@lonelyplanet/travel-agent": "^1.12.6",
    "aws-sdk": "^2.241.1",
    "backpack-ui": "^4.94.2",
    "commander": "^2.14.1",
    "diacritics": "^1.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "fetch-mock": "^6.0.1",
    "glob": "^7.1.2",
    "inversify": "^4.11.1",
    "isomorphic-fetch": "^2.2.1",
    "jest-cli": "22.4.4",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10",
    "radium": "^0.22.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-side-effect": "^1.1.5",
    "redux": "^4.0.0",
    "redux-actions": "^2.3.0",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.12",
    "ts-loader": "^4.2.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1"
  },
  "peerDependencies": {
    "backpack-ui": "^4.76.2",
    "react": "^0.14.0 || ^15.0.0-0 || ^16.0.0-0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/src/**/__tests__/**/*.(ts|tsx)"
    ],
    "setupFiles": [
      "./testSetup.js"
    ],
    "testURL": "http://localhost:3000"
  },
  "resolutions": {
    "@types/tapable": "1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.47",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "webpack-cli": "2.1.4",
    "webpack-dev-server": "^3.1.4"
  }
}
